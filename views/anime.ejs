<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <% animeSearch.forEach(data=>{%>

        <h2>
            <%= data.title %>
        </h2>
        <h3>
            <%= data.title_Japan%>
        </h3>
        <h6>
            <%= data.averageRating%>
        </h6>
        <h6>
            <%= data.date%>
        </h6>
        <img src="<%= data.image%>" alt="  <%= data.title %>">
        <h6>
            <%= data.gener_old%>
        </h6>
        <h6>
            <%= data.subtype%>
        </h6>
        <h6>
            <%= data.status%>
        </h6>
        <h6>
            <%= data.episodeCount%>
        </h6>
        <h6>
            <%= data.episodeLength%>
        </h6>
        <p>
            <%= data.synopsis%>
        </p>
        <iframe width="420" height="315" src="https://www.youtube.com/embed/<%= data.youtubeVideoId%>">
</iframe>

        <%}) %>
</body>

</html>